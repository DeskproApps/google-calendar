{
  "name": "@deskpro-apps/google-calendar",
  "title": "Google Calendar",
  "description": "View, create, and schedule future events with your users.",
  "version": "0.0.18",
  "scope": "agent",
  "appStoreUrl": "https://www.deskpro.com/product-embed/apps/google-calendar",
  "secrets": "",
  "hasDevMode": true,
  "isSingleInstall": false,
  "serveUrl": "https://apps-cdn.deskpro-service.com/__name__/__version__",
  "targets": [
    {
      "target": "ticket_sidebar",
      "entrypoint": "index.html"
    }
  ],
  "settings": {
    "use_deskpro_saas": {
      "title": "One-Click Installation",
      "type": "boolean",
      "default": true,
      "isRequired": false,
      "isBackendOnly": false,
      "order": 5
    },
    "client_id": {
      "title": "Client ID",
      "type": "string",
      "isRequired": false,
      "isBackendOnly": false,
      "condition": "settings.use_deskpro_saas != true",
      "order": 10
    },
    "client_secret": {
      "title": "Client Secret",
      "type": "string",
      "isRequired": false,
      "isBackendOnly": true,
      "condition": "settings.use_deskpro_saas != true",
      "order": 20
    },
    "callback_url": {
      "title": "Callback URL",
      "type": "app_embedded",
      "options": {
        "entrypoint": "#/admin/callback"
      },
      "isRequired": false,
      "isBackendOnly": true,
      "condition": "settings.use_deskpro_saas != true",
      "order": 30
    }
  },
  "proxy": {
    "whitelist": [
      {
        "url": "https://www.googleapis.com/calendar/v3/.*",
        "methods": [
          "GET",
          "POST"
        ],
        "timeout": 20
      },
      {
        "url": "https://oauth2.googleapis.com/.*",
        "methods": [
          "GET",
          "POST"
        ],
        "timeout": 20
      }
    ]
  }
}